{{#each events}}
  <div class="event-discr">
      <img src="{{images.url}}" alt="{{ name }}" width="180" height="227">
      <h2 class="event-title">{{ name }}</h2>
      <p class="event-text">{{ dates.start.localDate}}</p>
      <p class="event-text">{{ locale }} </p>
  </div>
{{/each}}


